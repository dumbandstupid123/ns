{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://your-backend-domain.railway.app/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
} 